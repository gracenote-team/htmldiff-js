  "main": "./dist/htmldiff.min.js",
  "author": "Jordan Hitch",
  "license": "ISC",
  "scripts": {
    "build": "webpack --mode production",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dfoverdx/htmldiff-js.git"
  },
  "keywords": [
